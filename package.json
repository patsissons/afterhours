{
  "name": "afterhours",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "pnpm": "^10",
    "node": "^22"
  },
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "conf": "pnpm run conf:link && pnpm run conf:env",
    "conf:link": "test -f .vercel/project.json || vercel link",
    "conf:env": "./scripts/setup-env.sh",
    "conf:clean": "rm -rf .vercel .env*.local",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ."
  },
  "dependencies": {
    "mongodb": "^4.7.0",
    "next": "^12.1.6",
    "next-auth": "^4.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@shopify/browserslist-config": "^3.0.0",
    "@shopify/eslint-plugin": "^41.2.1",
    "@shopify/prettier-config": "^1.1.2",
    "@shopify/react-testing": "^3.3.10",
    "@shopify/typescript-configs": "^5.1.0",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "eslint": "^8.18.0",
    "eslint-config-next": "^12.1.6",
    "prettier": "^2.6.2",
    "typescript": "^5.9.2",
    "vercel": "^25.1.0"
  },
  "browserslist": [
    "extends @shopify/browserslist-config"
  ]
}
